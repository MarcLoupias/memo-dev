        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MarcLoupias/memo-dev"
    },
    "bugs": {
        "url": "https://github.com/MarcLoupias/memo-dev/issues"
    },
    "homepage": "https://github.com/MarcLoupias/memo-dev",
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1"
    },
    "scripts": {
        "test": "markdownlint content/**/*.md",
        "prebuild": "mdfc convert 'mdfc-map-to-html' 'content/**/*.md' -o '../../../marked-options.js' && rimraf 'docs/'",
        "build": "cd content/ && find . -name '*.html' -or -name '*.png' -or -name '*.jpg' | cpio -pdm ../docs/ && find . -name '*.html' -type f -delete && cd .. && mv docs/SUMMARY.html docs/index.html && cp node_modules/github-markdown-css/github-markdown.css docs/github-markdown.css && cp -avr assets/. docs/",
        "postbuild": "parcel build docs/index.html --out-dir docs/",
        "serve": "http-server -c-1 docs/"
    },
    "license": "ISC",
    "devDependencies": {
        "github-markdown-css": "^3.0.1",
        "http-server": "^0.11.1",
        "markdownlint-cli": "^0.13.0",
        "mdfc-map-to-html": "^1.3.1",
        "parcel": "^1.11.0",
        "rimraf": "^2.6.3"
    },
    "dependencies": {
        "highlight.js": "^9.14.2"
    }
}
